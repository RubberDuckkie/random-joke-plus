{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-dupe-keys" : "warn",
        "no-tabs" : "warn",
        "no-param-reassign" : "warn",
        "no-plusplus" : "warn"
    }
}
